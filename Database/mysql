  custid INT AUTO_INCREMENT PRIMARY KEY,
  cfname VARCHAR(20),
  clname VARCHAR(20),
  caddress VARCHAR(60),
  cmob VARCHAR(15) NOT NULL UNIQUE,
  cemail VARCHAR(30) NOT NULL UNIQUE,
  cgender VARCHAR(15) NOT NULL,
  username VARCHAR(10) NOT NULL UNIQUE,
  password VARCHAR(255) NOT NULL check (
);

