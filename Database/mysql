CREATE TABLE classFair (
  Fair FLOAT,
  SeatType VARCHAR(15),
  FTrainNum INT,
  StartStnNum INT,
  EndStnNum INT,
  FOREIGN KEY (StartStnNum) REFERENCES stations(sid),
  FOREIGN KEY (EndStnNum) REFERENCES stations(sid),
  FOREIGN KEY (FTrainNum) REFERENCES trains(trn_no)
);

