  CREATE TABLE passenger_dtls (
  PNRno VARCHAR(10) NOT NULL PRIMARY KEY,
  user_id INT,
  destination VARCHAR(50),
  dest_stn_num INT,
  source_stn_num INT,
  start VARCHAR(50),
  trn_num INT,
  seat_no INT,
  booking_date DATETIME DEFAULT CURRENT_TIMESTAMP,
  username VARCHAR(10)
  );

  drop table passenger_dtls;
  
